      <Get Name="Get" Id="{687dfcbd-6ad8-4ffb-a3c4-66b35289f9de}">
        <Declaration><![CDATA[VAR
END_VAR
]]></Declaration>
        <Implementation>
          <ST><![CDATA[IF bInitOk THEN
  fCameraToPrintOffset := _pCamWorker^.fCameraToPrintOffset;
END_IF]]></ST>
        </Implementation>
      </Get>
    </Property>
    <Property Name="fDetectToCamera" Id="{0268497d-c5b0-4639-a734-da6955c88aff}" FolderPath="I_ParamMediator\">
