    <Method Name="StartDeformation" Id="{197196c8-f164-46d2-868e-8fff7b370e59}">
      <Declaration><![CDATA[METHOD StartDeformation
VAR_INPUT
  eDeformationStep : E_DeformationStep;
  //nParam1 : UDINT;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF eDeformationStep = E_DeformationStep.nothing THEN
  ; // do nothing
ELSIF eDeformationStep = E_DeformationStep.createdScriptContainer THEN
  ;
ELSIF eDeformationStep = E_DeformationStep.cameraStarted THEN
  fBImageDeformation.startCamera(
    fStartDefPos:=DINT_TO_LREAL(THIS^.GetPos(nPosDomain:=E_PosDomain.pos_px))
  );
END_IF


// update step if it is bigger
IF fBImageDeformation.eStep < eDeformationStep THEN
  fBImageDeformation.eStep := eDeformationStep;
  LogLvl(Lvl:=E_LogSeverity.Debug, Msg:=F_Concat2('StartDeformation: step=', TO_STRING(eDeformationStep)));
END_IF
]]></ST>
      </Implementation>
    </Method>
    <Method Name="UpdateDeformation" Id="{cb380da5-bcaa-4509-a9f1-3ad6d51f2b1a}">
      <Declaration><![CDATA[(*
  Updates deformation module (max. in given interval)
*)
METHOD PUBLIC UpdateDeformation : BOOL
VAR_INPUT
  fUpdateInterval : LREAL;  // deformation module update interval [px]
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[fBImageDeformation.updateDeformation(
  fPos:=THIS^.GetPos(nPosDomain:=E_PosDomain.pos_px), 
  fUpdateInterval:=fUpdateInterval
);]]></ST>
      </Implementation>
    </Method>
