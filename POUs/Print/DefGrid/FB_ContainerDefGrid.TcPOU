    <Method Name="SetupDeformation" Id="{fa9cfc27-a9f9-4b08-a02d-72b82e2069db}">
      <Declaration><![CDATA[METHOD SetupDeformation
VAR_INPUT
  eCameraMode : E_CameraMode;
END_VAR]]></Declaration>
      <Implementation>
