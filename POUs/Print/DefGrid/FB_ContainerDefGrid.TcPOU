    <Method Name="getDeformationPoint" Id="{2c7c8e7c-37b2-43e1-90a8-ea784d68621e}">
      <Declaration><![CDATA[(*
  get a deformation point.
  :returns: TRUE if successful
*)
METHOD getDeformationPoint : BOOL;
VAR_INPUT
  nCol : UDINT;   // col select
  nRow : UDINT;   // row select
END_VAR
VAR_OUTPUT
