    </Implementation>
    <Folder Name="I_ParamMediator" Id="{39f9222d-d28f-4df1-8ed1-e2b3d113f34f}" />
    <Method Name="ClearCachedCT" Id="{d54eef11-94fc-4211-8079-b54bc203d3ec}" FolderPath="I_ParamMediator\">
      <Declaration><![CDATA[(*
  Release deformation ct
  TODO: find another way to do inter worker communication for generic methods.
*)
METHOD ClearCachedCT
VAR_INPUT
  iCT              : I_CT;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF bInitOk THEN
  _pCreateWorker^.DeleteDefCt(iCT:=iCT);
END_IF]]></ST>
      </Implementation>
    </Method>
    <Property Name="fCameraToPrintOffset" Id="{43edb161-94bb-440e-a146-2ecc3ab0b15d}" FolderPath="I_ParamMediator\">
