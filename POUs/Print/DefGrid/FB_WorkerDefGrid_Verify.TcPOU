    <Method Name="setupPosition" Id="{ba790993-47df-42cc-8f54-d5be88ee8b37}">
      <Declaration><![CDATA[METHOD setupPosition
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF _pAttachedCt <> 0 THEN RETURN; END_IF    // Update only without attached container

fPosition := _fWorkerToRefPos + _fPmPosition;     // generic module specific "offset" + configurative offset
]]></ST>
      </Implementation>
    </Method>
    <Method Name="work" Id="{f3293130-827d-4f71-beb2-d681c617967b}">
      <Declaration><![CDATA[METHOD  work
VAR_INST
  fT_Pulse  : F_TRIG;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[SUPER^.work();

setupPosition();    // Worker position update

IF _pAttachedCt <> 0 THEN
  tpEjectPulse(Q=>bWateGateActive);
  fT_Pulse(CLK:=bWateGateActive);
  
  // release
  IF tpEjectPulse.IN THEN
    IF fT_Pulse.Q THEN  
      tpEjectPulse(IN:=FALSE);
      LogLvl(Lvl:=E_LogSeverity.Debug, Msg:=F_Concat2('Eject CT: ', _pAttachedCt^.print(0)));
      Release(iCT:=_pAttachedCt^);
    END_IF
  ELSE
    Release(iCT:=_pAttachedCt^);
  END_IF
END_IF]]></ST>
      </Implementation>
    </Method>
